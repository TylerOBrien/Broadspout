{
    "compilerOptions": {
      "baseUrl": "./",
      "outDir": "./dist/",
      "target": "es2017",
      "lib": ["es2015", "es2016", "es2017", "es6", "es7", "es5", "dom"],
      "jsx": "react",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "allowJs": true,
      "paths": {
        "@config/*": ["./config/*"],
        "@resources/*": ["./resources/*"],
        "@system/*": ["./system/*"],
      }
    },
    "exclude": ["node_modules", "dist"]
  }
